    "file-loader": "^6.2.0",
    "framer-motion": "^4.1.5",
    "hardhat": "^2.9.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-is": "^18.0.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
